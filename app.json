{
  "expo": {
    "name": "ExpoFaceDetectorDemo",
    "slug": "ExpoFaceDetectorDemo",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo-sm.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 10000
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "reactnative.expo.facedector.demo",
      "buildNumber": "1.0.0",
      "infoPlist": {
        "NSCameraUsageDescription": "顔写真撮影のためにカメラを使用します。",
        "NSPhotoLibraryUsageDescription": "Use the photo library to change your avatar.",
        "CFBundleDevelopmentRegion": "ja_JP"
      },
      "userInterfaceStyle": "automatic"
    },
    "android": {
      "package": "reactnative.expo.facedector.demo",
      "versionCode": 1,
      "userInterfaceStyle": "automatic",
      "permissions": [
        "VIBRATE",
        "CAMERA"
      ]
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "js",
        "jsx",
        "svg",
        "ts",
        "tsx"
      ]
    },
    "extra": {
      "eas": {
        "projectId": "7ee49ceb-3646-43b2-9179-a8bb74767909"
      }
    }
  }
}
